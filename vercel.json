{
  "images": {
    "sizes": [140, 160, 400, 500, 600, 800],
    "domains": ["soundraiser.io", "lovable-uploads.vercel.app", "owtufhdsuuyrgmxytclj.supabase.co"],
    "formats": ["image/webp"],
    "minimumCacheTTL": 60,
    "dangerouslyAllowSVG": true,
    "contentSecurityPolicy": "default-src 'self'; script-src 'none'; sandbox;"
  },
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "^/lovable-uploads/(.*)",
      "dest": "/_next/image?url=/lovable-uploads/$1&w=1080&q=80"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/_vercel/image(.*)",
      "dest": "/_vercel/image$1"
    },
    {
      "src": "/((?!api|static|_next|lovable-uploads|assets).*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(jpg|jpeg|png|webp|avif|svg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
